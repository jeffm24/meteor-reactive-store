{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "max-len": 0,
        "no-param-reassign": 0,
        "one-var": 0,
        "no-continue": 0,
        "comma-dangle": 0,
        "no-plusplus": 0,
        "semi-spacing": 0,
        "no-restricted-syntax": 0,
        "one-var-declaration-per-line": 0,
        "no-restricted-properties": 0,
        "consistent-return": 0,
        "no-else-return": 0,
        "padded-blocks": 0,
        "quote-props": 0,
        "no-console": 0,
        "func-names": 0,
        "radix": 0,
        "object-shorthand": 0,
        "no-useless-escape": 0,
        "no-trailing-spaces": 0,
        "arrow-body-style": 0,
        "no-bitwise": 0,
        "import/no-absolute-path": 0,
        "import/prefer-default-export": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "linebreak-style": 0,
        "new-cap": 0,
        "class-methods-use-this": 0,
        "no-use-before-define": ["error", { "functions": false }],
        "no-prototype-builtins": 0
    },
    "globals": {
        "Package": true,
        "describe": true,
        "it": true
    }
}